{
  "name": "catjammer",
  "version": "0.1.0",
  "description": "catJAM Discord Bot",
  "main": "index.js",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "test": "mocha",
    "coverage": "nyc --use-spawn-wrap mocha",
    "lint:full": "eslint \"./{src,test}/**/*.ts\"",
    "lint:quick": "eslint --cache \"./{src,test}/**/*.ts\"",
    "lint:fix": "eslint --fix \"./{src,test}/**/*.ts\""
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "@types/node-schedule": "^1.3.0",
    "@types/sinon": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "rewiremock": "^3.14.3",
    "sinon": "^9.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "nyc": {
    "all": true,
    "reporter": [
      "lcov",
      "text"
    ],
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**"
    ],
    "exclude": [
      "benchmarks/**",
      "dist/**",
      "test/**",
      "src/index.ts",
      "**/.*.js"
    ]
  },
  "dependencies": {
    "discord.js": "^12.3.1",
    "node-schedule": "^1.3.2",
    "rxjs": "^6.6.3"
  }
}
