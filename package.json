{
  "name": "catjammer",
  "version": "0.1.0",
  "description": "catJAM Discord Bot",
  "main": "index.js",
  "scripts": {
    "build": "tsc --build ./tsconfig.production.json",
    "start": "ts-node --files ./src/index.ts",
    "start:prod": "ts-node -T --files ./src/index.ts",
    "test": "mocha",
    "coverage": "nyc --use-spawn-wrap mocha",
    "lint:full": "eslint \"./{src,test}/**/*.ts\"",
    "lint:quick": "eslint --cache \"./{src,test}/**/*.ts\"",
    "lint:fix": "eslint --fix \"./{src,test}/**/*.ts\""
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "chai": "^4.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "sinon": "^9.2.1",
    "ts-node": "^9.0.0"
  },
  "nyc": {
    "all": true,
    "reporter": [
      "lcov",
      "text"
    ],
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**"
    ],
    "exclude": [
      "benchmarks/**",
      "dist/**",
      "test/**",
      "src/index.ts",
      "**/.*.js"
    ]
  },
  "dependencies": {
    "@keyv/sqlite": "^2.0.2",
    "@types/chai": "^4.2.14",
    "@types/keyv": "^3.1.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.6",
    "@types/node-schedule": "^1.3.1",
    "@types/sinon": "^9.0.8",
    "@types/ws": "^7.2.9",
    "discord.js": "^12.4.1",
    "keyv": "^4.0.3",
    "node-schedule": "^1.3.2",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  }
}
